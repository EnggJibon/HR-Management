
<a id="signOutLink" class="dropdown-toggle" href="javascript:void(0)" data-toggle="">Sign Out</a>
<div id="logout-url" data-request-url="@Url.Action("LogOut", "General")"></div>

<script type="text/javascript">
    $(document).ready(function () {
        $('#signOutLink').click(function () {
            $.ajax({
                type: "POST",
                url: "/General/LogOut",
                cache: false,
                success: function (data) {
                    window.location = data.url;
                    //location.reload("Index","Home");
                },
                error: function () {
                }
            });
        });
    });
</script>