<script type="text/javascript">
    $(document).ready(function () {
        $('#signOutLink').click(function () {

            $.ajax({
                type: "POST",
                url: '@Url.Action("LogOut", "General")',
                cache: false,
                success: function (data) {

                    window.location = data.url;
                    //location.reload("Index","Home");
                },
                error: function () {
                }
            });
        });


    });
</script>